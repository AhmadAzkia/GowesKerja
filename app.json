{
  "expo": {
    "name": "GowesKerja",
    "slug": "goweskerja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "goweskerja",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.ahmadazkia.goweskerja",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyACJ6koRoZ6NdsB2oJRvOs04UwOrlJmymM"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyACJ6koRoZ6NdsB2oJRvOs04UwOrlJmymM"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location when open and in the background."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
